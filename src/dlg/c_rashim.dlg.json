        "Text": {
          "type": "cexolocstring",
          "value": {
            "0": "*The Grandmaster of the Frozen Hand stares as you approach, his hardened gaze chilling every breath you exhale in his presence.*"
          }
        }
      },
      {
        "__struct_id": 1,
        "Animation": {
          "type": "dword",
          "value": 0
        },
        "AnimLoop": {
          "type": "byte",
          "value": 1
        },
        "Comment": {
          "type": "cexostring",
          "value": ""
        },
        "Delay": {
          "type": "dword",
