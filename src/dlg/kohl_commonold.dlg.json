        "Text": {
          "type": "cexolocstring",
          "value": {
            "0": "Those young brats trying to bait that nymph from the Feylake ruined my walk last night with their vulgar poems. Their mothers should wash their mouths out with soap for such language."
          }
        }
      },
      {
        "__struct_id": 1,
        "Animation": {
          "type": "dword",
          "value": 0
        },
        "AnimLoop": {
          "type": "byte",
