        },
        "Text": {
          "type": "cexolocstring",
          "value": {
            "0": "Let me tell you a story!"
          }
        }
      },
      {
        "__struct_id": 3,
        "Animation": {
          "type": "dword",
          "value": 0
        },
        "AnimLoop": {
